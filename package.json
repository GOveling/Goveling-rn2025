{
  "name": "goveling-native-expo",
  "version": "0.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --port 8081",
    "dev": "expo start --port 8081",
    "dev:native": "expo start --port 8081",
    "dev:web": "expo start --web --port 8081",
    "dev:tunnel": "expo start --tunnel --port 8081",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web --port 8081",
    "build": "expo export --platform web",
    "build:web": "expo export --platform web",
    "web:build": "expo export --platform web",
    "setup": "echo 'run supabase db push && supabase functions deploy'",
    "setup-api-keys": "./setup-api-keys.sh",
    "security-check": "echo 'üîç Verificando exposici√≥n de API keys...' && grep -r 'AIza\\|sk-\\|pk\\.' src/ --exclude-dir=node_modules || echo '‚úÖ No se encontraron API keys expuestas'",
    "config-check": "node -e \"require('./src/config/apiKeys.ts')\"",
    "prebuild": "expo prebuild",
    "pods": "npx pod-install",
    "dev:all": "expo start --port 8081",
    "update": "eas update --auto",
    "update:prod": "eas update --auto --branch production",
    "update:dev": "eas update --auto --branch development",
    "deploy": "git add . && git commit -m 'feat: update app' && git push && eas update --auto",
    "doctor": "expo doctor",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:geo": "jest src/lib/geo/__tests__",
    "prepare": "husky install || true"
  },
