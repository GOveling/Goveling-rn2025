{
  "expo": {
    "name": "Goveling",
    "slug": "goveling-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./assets/favicon.ico"
    },
    "splash": {
      "image": "./assets/branding-zeppeling.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "goveling",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-notifications",
      "expo-location",
      "expo-image-picker",
      "expo-document-picker",
      "expo-file-system",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      "expo-updates",
      [
        "react-native-maps",
        {
          "useAppleMapKit": true,
          "useGoogleMaps": false
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.goveling.app",
      "supportsTablet": false,
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch",
          "remote-notification",
          "location",
          "fetch",
          "remote-notification"
        ],
        "NSLocationWhenInUseUsageDescription": "Esta app necesita acceso a tu ubicaci칩n para mostrar lugares cercanos y direcciones.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Esta app necesita acceso a tu ubicaci칩n para mostrar lugares cercanos y direcciones.",
        "NSCameraUsageDescription": "Esta app necesita acceso a la c치mara para tomar fotos.",
        "NSPhotoLibraryUsageDescription": "Esta app necesita acceso a tus fotos para seleccionar im치genes."
      }
    },
    "android": {
      "package": "com.goveling.app",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "082d74f5-5463-424d-b8a3-0f2330c03846"
      }
    },
    "owner": "goveling",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/082d74f5-5463-424d-b8a3-0f2330c03846"
    }
  }
}
